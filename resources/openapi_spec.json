    "/v2/lat/{lat}/lon/{lon}/dist/{radius}": {
      "get": {
        "tags": [
          "v2"
        ],
        "summary": "Aircrafts surrounding a point (lat, lon) up to 250nm",
        "description": "Returns aircraft located in a circle described by the latitude and longtidude of its center and its radius.",
        "operationId": "v2_point_v2_lat__lat__lon__lon__dist__radius__get",
        "parameters": [
          {
            "name": "lat",
            "in": "path",
            "required": true,
            "schema": {
              "type": "number",
              "maximum": 90,
              "minimum": -90,
              "title": "Lat"
            },
            "example": 51.89508
          },
          {
            "name": "lon",
            "in": "path",
            "required": true,
            "schema": {
              "type": "number",
              "maximum": 180,
              "minimum": -180,
              "title": "Lon"
            },
            "example": 2.79437
          },
          {
            "name": "radius",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "maximum": 250,
              "minimum": 0,
              "title": "Radius"
            },
            "example": 250
